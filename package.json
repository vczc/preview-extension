{
  "name": "zkos-studio-plugin",
  "displayName": "zkos-studio-plugin",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.78.0"
  },
  "publisher": "Zeekr",
  "categories": [
    "Other"
  ],
  "license": "SEE LICENSE IN LICENSE",
  "icon": "images/logo.png",
  "activationEvents": [],
  "extensionDependencies": [
    "toolchain.Tortie-preview"
  ],
  "main": "./out/extension.js",
  "scripts": {
    "dev": "vite",
    "build:static": "vite build",
    "esbuild-base": "esbuild ./src-extension/extension.ts --bundle --outfile=out/extension.js --external:vscode --format=cjs --platform=node --sourcemap",
    "esbuild": "npm run esbuild-base -- --sourcemap",
    "esbuild-watch": "npm run esbuild-base -- --sourcemap --watch",
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "npm run build:static && NODE_ENV=development npm run esbuild-base -- --sourcemap --watch",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.8.0",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vscode/test-electron": "^2.3.0",
    "@vscode/vsce": "^2.18.0",
    "esbuild": "^0.17.19",
    "eslint": "^8.39.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "sass": "^1.62.1",
    "typescript": "^5.0.4",
    "vite": "^4.2.0",
    "vue-tsc": "^1.2.0"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.1.0",
    "@types/node": "^18.15.11",
    "axios": "^1.4.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "element-plus": "2.3.0",
    "lodash": "^4.17.21",
    "nanoid": "^4.0.2",
    "pinia": "^2.1.3",
    "sudo-prompt": "^9.2.1",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6",
    "vuex": "^4.1.0",
    "winston": "^3.8.2",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "type": "commonjs",
  "contributes": {
    "configuration": {
      "title": "zop-plugin",
      "properties": {
        "Zoks: Global SDK Path": {
          "type": "string",
          "default": "",
          "description": "请设置你的SDK目录"
        }
      }
    },
    "webviews": [
			{
				"match": "http://10.114.149.125:15000/service_demo/#/service/service_design",
				"name": "test",
				"webviewContentSecurityPolicy": "default-src 'self' http://10.114.149.125:15000/service_demo/#/service/service_design; frame-src http://10.114.149.125:15000/service_demo/#/service/service_design"
			}
		],
    "commands": [
      {
        "command": "zopPlugin.addNewNode",
        "title": "添加配置"
      },
      {
        "command": "zopPlugin.renameNode",
        "title": "修改名称"
      },
      {
        "command": "zopPlugin.removeNode",
        "title": "删除"
      },
      {
        "command": "Build_setting_click",
        "title": "打开配置页面"
      },
      {
        "command": "view.refresh",
        "title": "刷新",
        "icon": {
          "dark": "images/dark/dark_refresh.png",
          "light": "images/light/light_refresh.png"
        }
      },
      {
        "command": "toolChain.click",
        "title": "工具链"
      },
      {
        "command": "serviceCheck.click",
        "title": "服务验证"
      },
      {
        "command": "zopPlugin.delCache",
        "title": "清理缓存"
      }
    ],
    "menus": {
      "editor/context": [],
      "view/title": [
        {
          "command": "view.refresh",
          "when": "view == zopCompileDebugView",
          "group": "navigation"
        }
      ],
      "view/item/context": [
        {
          "command": "zopPlugin.addNewNode",
          "when": "(view == zopCodeGenerate || view == zopCompileDebugView) && (viewItem == Build_setting)"
        },
        {
          "command": "zopPlugin.renameNode",
          "when": "(view == zopCodeGenerate || view == zopCompileDebugView) && (viewItem == Build_setting_child)"
        },
        {
          "command": "zopPlugin.removeNode",
          "when": "(view == zopCodeGenerate || view == zopCompileDebugView) && (viewItem == Build_setting_child)"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "zopview",
          "title": "zkos ide",
          "icon": "images/dep.png"
        }
      ]
    },
    "views": {
      "zopview": [
        {
          "id": "zopCodeGenerate",
          "name": "代码生成工具"
        },
        {
          "id": "zopCompileDebugView",
          "name": "编译调试工具"
        },
        {
          "id": "zopCheckDebugToolView",
          "name": "验证调试工具"
        },
        {
          "id": "zopSetting",
          "name": "设置"
        },
        {
          "id": "zopHelpFeedbackView",
          "name": "帮助和反馈"
        }
      ]
    },
    "viewsWelcome": [
      {
        "view": "zopHelpFeedbackView",
        "contents": "简介: ZKOS SDK使用帮助\n当前版本: v0.0.1\n[$(preview)zkos主页](https://zkos.zeekrlife.com/zh/home/)\n"
      }
    ]
  }
}
